msgid "ROKSPROCKET_QUOTES_LAYOUT_TITLE"
msgstr "Quotes Layout Options"

msgid "ROKSPROCKET_QUOTES_LAYOUT_DEFAULTS_TITLE"
msgstr "Quotes Defaults"

msgid "ROKSPROCKET_DEFAULT_QUOTE_TEXT"
msgstr "Default Quote Text"

msgid "ROKSPROCKET_DEFAULT_QUOTE_TEXT_DESC"
msgstr ""
"You can select a default Quote text for all articles based on this field."

msgid "ROKSPROCKET_QUOTES_ITEMSPERPAGE_LABEL"
msgstr "Previews per Page"

msgid "ROKSPROCKET_QUOTES_ITEMSPERPAGE_DESC"
msgstr "Amount of article previews to show on each page"

msgid "ROKSPROCKET_QUOTES_ITEMSPERROW"
msgstr "Items Per Row"

msgid "ROKSPROCKET_QUOTES_ITEMSPERROW_DESC"
msgstr "Amount of article items to show in each row."

msgid "ROKSPROCKET_LAYOUT_QUOTES_TITLE_LABEL"
msgstr "Title"

msgid "ROKSPROCKET_LAYOUT_QUOTES_TITLE_DESC"
msgstr ""
"The title to be used for the List item. If not set, it will default to the "
"article title."

msgid "ROKSPROCKET_LAYOUT_QUOTES_DESCRIPTION_LABEL"
msgstr "Quote Text"

msgid "ROKSPROCKET_LAYOUT_QUOTES_DESCRIPTION_DESC"
msgstr ""
"The text to be used for the quote. If not set, it will defaults to the "
"article Introtext."

msgid "ROKSPROCKET_LAYOUT_QUOTES_AUTHOR_LABEL"
msgstr "Author"

msgid "ROKSPROCKET_LAYOUT_QUOTES_AUTHOR_DESC"
msgstr "The name of the person or entity the quote is being attributed to."

msgid "ROKSPROCKET_LAYOUT_QUOTES_SUBTEXT_LABEL"
msgstr "Subtext"

msgid "ROKSPROCKET_LAYOUT_QUOTES_SUBTEXT_DESC"
msgstr ""
"Additional credits or organization information to appear beneath the quote "
"author name."

msgid "ROKSPROCKET_LAYOUT_QUOTES_IMAGE_LABEL"
msgstr "Image"

msgid "ROKSPROCKET_LAYOUT_QUOTES_IMAGE_DESC"
msgstr "The image that will appear alongside the author of the quote."

msgid "ROKSPROCKET_LAYOUT_QUOTES_LINK_LABEL"
msgstr "Link"

msgid "ROKSPROCKET_LAYOUT_QUOTES_LINK_DESC"
msgstr ""
"If set, the link will show a 'Read More' button, link the title and based on "
"the Theme, link the image as well."

msgid "ROKSPROCKET_LAYOUT_QUOTES_DIRECTION_LABEL"
msgstr "Direction"

msgid "ROKSPROCKET_LAYOUT_QUOTES_DIRECTION_DESC"
msgstr "Specify the orientation of the quote bubble arrow."

msgid "SLIDE"
msgstr "Slide"

msgid "FADEDELAY"
msgstr "Fade Delayed"

msgid "FLYIN"
msgstr "Fly In"

msgid "FALLDOWN"
msgstr "Fall Down"

msgid "FLOATUP"
msgstr "Float Up"

msgid "SCALEIN"
msgstr "Scale In"

msgid "SCALEOUT"
msgstr "Scale Out"

msgid "SINGLE_ROW"
msgstr "Single Row"

msgid "WRAP"
msgstr "Wrap"

msgid "TOP_LEFT"
msgstr "Top Left"

msgid "TOP_RIGHT"
msgstr "Top Right"

msgid "BOTTOM_LEFT"
msgstr "Bottom Left"

msgid "BOTTOM_RIGHT"
msgstr "Bottom Right"

msgid "NO_ARROW"
msgstr "No Arrow"
