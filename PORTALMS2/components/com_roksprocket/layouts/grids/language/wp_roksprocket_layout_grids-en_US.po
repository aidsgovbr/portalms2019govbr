msgid "ROKSPROCKET_GRIDS_LAYOUT_TITLE"
msgstr "Grids Layout Options"

msgid "ROKSPROCKET_GRIDS_LAYOUT_DEFAULTS_TITLE"
msgstr "Grids Article Defaults"

msgid "ROKSPROCKET_GRIDS_COLUMNS"
msgstr "Columns"

msgid "ROKSPROCKET_GRIDS_COLUMNS_DESC"
msgstr "Amount of columns the blocks should be layed down like"

msgid "ROKSPROCKET_GRIDS_ANIMATIONS"
msgstr "Block Animation"

msgid "ROKSPROCKET_GRIDS_ANIMATIONS_DESC"
msgstr ""
"The types of animations desired for the blocks when appearing/disappearing. "
"Default is Fade."

msgid "ROKSPROCKET_GRIDS_ANIMATIONS_FADE"
msgstr "Fade"

msgid "ROKSPROCKET_GRIDS_ANIMATIONS_SCALE"
msgstr "Scale"

msgid "ROKSPROCKET_GRIDS_ANIMATIONS_ROTATE"
msgstr "Rotate"

msgid "ROKSPROCKET_GRIDS_ANIMATIONS_FLIP"
msgstr "Flip"

msgid "ROKSPROCKET_LAYOUT_GRIDS_TITLE_LABEL"
msgstr "Title"

msgid "ROKSPROCKET_LAYOUT_GRIDS_TITLE_DESC"
msgstr ""
"The title to be used for the Grids item. If not set, it will default to the "
"article title."

msgid "ROKSPROCKET_LAYOUT_GRIDS_DESCRIPTION_LABEL"
msgstr "Description"

msgid "ROKSPROCKET_LAYOUT_GRIDS_DESCRIPTION_DESC"
msgstr ""
"The description to be used for the Feature. If not set, it will defaults to "
"the article Introtext."

msgid "ROKSPROCKET_LAYOUT_GRIDS_IMAGE_LABEL"
msgstr "Image"

msgid "ROKSPROCKET_LAYOUT_GRIDS_IMAGE_DESC"
msgstr "The image to be used assigned to the article for the Grids items."

msgid "ROKSPROCKET_LAYOUT_GRIDS_LINK_LABEL"
msgstr "Link"

msgid "ROKSPROCKET_LAYOUT_GRIDS_LINK_DESC"
msgstr ""
"If set, the link will show a 'Read More' button, link the title and based on "
"the Theme, link the image as well."

msgid "ALL"
msgstr "All"
